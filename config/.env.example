# Overlord Trading System v9 - Environment Variables Template
# Copy this file to .env and fill in your credentials

# Binance API Credentials
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here

# Coinbase API Credentials
COINBASE_API_KEY=your_coinbase_api_key_here
COINBASE_API_SECRET=your_coinbase_api_secret_here

# Bybit API Credentials (optional)
BYBIT_API_KEY=
BYBIT_API_SECRET=

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/overlord_db
REDIS_URL=redis://localhost:6379/0

# System Configuration
SYSTEM_MODE=standard  # dry-run | conservative | standard | aggressive
LOG_LEVEL=INFO
ENVIRONMENT=production

# Monitoring & Alerting
SLACK_WEBHOOK_URL=
DATADOG_API_KEY=

# Security
JWT_SECRET_KEY=your_jwt_secret_key_here
API_KEY_ROTATION_DAYS=90
